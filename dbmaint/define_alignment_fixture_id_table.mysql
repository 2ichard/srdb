create table alignment_fixture_ids (
	id MEDIUMINT KEY AUTO_INCREMENT,
	t TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
	seg VARCHAR(8) NOT NULL,
	proc VARCHAR(8) NOT NULL,
	ver VARCHAR(8) NOT NULL,
	sec TINYINT NOT NULL,
	step TINYINT NOT NULL,
	point VARCHAR(8) NOT NULL,
	fixture_id VARCHAR(16) NOT NULL
);
